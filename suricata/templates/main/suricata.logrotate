# Sample /etc/logrotate.d/suricata configuration file.
/var/log/suricata/*.log /var/log/suricata/*.json {
        daily
        missingok
        rotate 5
        compress
        delaycompress
        minsize 500k
        sharedscripts
        postrotate
            /bin/kill -HUP `cat /var/run/suricata.pid 2> /dev/null` 2> /dev/null || true
        endscript
}
